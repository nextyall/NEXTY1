{
  "name": "WhatsApp 2X Bot",
  "description": "High-performance WhatsApp bot on Heroku 2X Basic Plan",
  "repository": "https://github.com/nextyall/NEXTY1",
  "logo": "https://telegra.ph/file/64c4e67f38e76dcabead8.jpg",
  "keywords": [
    "whatsapp",
    "bot",
    "nodejs",
    "heroku",
    "2x-plan"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jontewks/puppeteer-heroku-buildpack"
    }
  ],
  "env": {
    "SESSION_ID": {
      "description": "Get Session ID from flex-pairing",
      "value": "REPLACE_WITH_YOUR_SESSION_ID",
      "required": true
    },
    "NODE_ENV": {
      "description": "Environment type",
      "value": "production",
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
