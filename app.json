{
  "name": "WhatsApp Bot",
  "description": "A WhatsApp bot with menu, ping, JID and message forwarding features",
  "repository": "https://github.com/nextyall/NEXTY1",
  "logo": "https://telegra.ph/file/64c4e67f38e76dcabead8.jpg",
  "keywords": [
    "whatsapp",
    "bot",
    "nodejs",
    "heroku"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jontewks/puppeteer-heroku-buildpack"
    }
  ],
  "env": {
    "SESSION_ID": {
      "description": "Get Session ID from: https://flex-pairing2-4ea9250ad369.herokuapp.com/pair",
      "value": "REPLACE_WITH_YOUR_SESSION_ID",
      "required": true
    },
    "NODE_ENV": {
      "description": "Environment type",
      "value": "production",
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "eco"
    }
  }
}
